  },
  "homepage": "https://wardpearce.com/dathost",
  "dependencies": {
    "axios": "^0.21.1",
    "steamid": "^1.1.3"
  },
  "devDependencies": {
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "chai": "^4.3.4",
    "esbuild": "^0.11.20",
    "eslint": "^7.25.0",
    "mocha": "^8.4.0",
    "ts-node-register": "^1.0.0",
    "typescript": "^4.2.4"
  }

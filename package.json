  "description": "TypeScript wrapper for Dathost's API",
  "main": "src/index.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint src/** --ext .js,.jsx,.ts,.tsx",

  "license": "GPL v3",
  "bugs": {
    "url": "https://github.com/UnofficialDathost/TypeScript/issues"
  },
  "homepage": "https://dathost.wardpearce.com/ts/docs",
  "dependencies": {
    "@types/cuint": "^0.2.1",
    "axios": "^0.21.1",
